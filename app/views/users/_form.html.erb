    		<div class="row">  	
	    		<div class="col-sm-8">
	    			<div class="contact-form">
                        
                        <form id="main-contact-form" class="contact-form row" name="contact-form" method="post">
                        
                        
  <div class="form-group col-md-6">
    <%= form.label 'E-mail' %>
    <%= form.text_field :email, id: :user_useremail, class: 'form-control'%>
  </div>

  <div class="form-group col-md-6">
    <%= form.label 'Password' %>
    <%= form.text_field :password, id: :user_userpassword, class: 'form-control' %>
  </div>

  <div class="form-group col-md-6">
    <%= form.label 'First Name' %>
    <%= form.text_field :userfirstname, id: :user_userfirstname, class: 'form-control' %>
  </div>

  <div class="form-group col-md-6">
    <%= form.label 'Last Name' %>
    <%= form.text_field :userlastname, id: :user_userlastname, class: 'form-control' %>
  </div>

  <div class="form-group col-md-6">
    <%= form.label 'City' %>
    <%= form.text_field :usercity, id: :user_usercity, class: 'form-control' %>
  </div>

  <div class="form-group col-md-6">
    <%= form.label 'Phone Number' %>
    <%= form.text_field :userphone, id: :user_userphone, class: 'form-control' %>
  </div>

  <div class="form-group col-md-12">
    <%= form.label :Address %>
    <%= form.text_field :useraddress, id: :user_useraddress, class: 'form-control' %>
  </div>

  <div class="form-group col-md-12">
    <%= form.submit %>
  </div>
                            </form>
                    </div>
                </div>
</div>
<% end %>